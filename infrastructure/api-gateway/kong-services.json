{
  "services": [
    {
      "name": "product-api",
      "url": "http://product-api:8080",
      "routes": [
        {
          "name": "product-route",
          "paths": ["/api/product"],
          "plugins": [
            {
              "name": "jwt"
            },
            {
              "name": "rate-limiting",
              "config": {
                "minute": 60,
                "policy": "local"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "order-api",
      "url": "http://order-api:8080",
      "routes": [
        {
          "name": "order-route",
          "paths": ["/api/order"]
        }
      ]
    }
  ]
}
